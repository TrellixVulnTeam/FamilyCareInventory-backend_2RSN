{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    /* eslint-disable */\n\n    const store = useStore();\n    const activeName = ref('first');\n    const form = ref({});\n    const rules = ref({\n      username: [{\n        required: true,\n        message: 'Please input email/phone',\n        trigger: 'blur'\n      }],\n      password: [{\n        required: true,\n        message: 'Please input password',\n        trigger: 'blur'\n      }]\n    });\n    const formRef = ref(null);\n\n    const handleLogin = () => {\n      formRef.value.validate(async valid => {\n        if (valid) {\n          console.log('valid!'); // console.log(form.value)\n          // await login(form.value)\n\n          store.dispatch('app/login', form.value);\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    };\n\n    const focusOnPassword = () => {\n      var password = document.getElementById('password');\n      password.focus();\n    };\n\n    const __returned__ = {\n      store,\n      activeName,\n      form,\n      rules,\n      formRef,\n      handleLogin,\n      focusOnPassword,\n      ref,\n      useStore\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAkCA,SAASA,GAAT,QAAoB,KAApB;AACA,SAASC,QAAT,QAAyB,MAAzB;;;;;;IAFA;;IAGA,MAAMC,KAAK,GAAGD,QAAQ,EAAtB;IACA,MAAME,UAAU,GAAGH,GAAG,CAAC,OAAD,CAAtB;IACA,MAAMI,IAAI,GAAGJ,GAAG,CAAC,EAAD,CAAhB;IAGA,MAAMK,KAAK,GAAGL,GAAG,CAAC;MAChBM,QAAQ,EAAE,CACR;QACEC,QAAQ,EAAE,IADZ;QAEEC,OAAO,EAAE,0BAFX;QAGEC,OAAO,EAAE;MAHX,CADQ,CADM;MAQhBC,QAAQ,EAAE,CACR;QACEH,QAAQ,EAAE,IADZ;QAEEC,OAAO,EAAE,uBAFX;QAGEC,OAAO,EAAE;MAHX,CADQ;IARM,CAAD,CAAjB;IAgBA,MAAME,OAAO,GAAGX,GAAG,CAAC,IAAD,CAAnB;;IACA,MAAMY,WAAW,GAAG,MAAM;MACxBD,OAAO,CAACE,KAAR,CAAcC,QAAd,CAAuB