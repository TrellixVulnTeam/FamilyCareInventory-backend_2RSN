{"ast":null,"code":"import { onMounted, ref } from 'vue';\nimport { useStore } from 'vuex';\nimport { myItems } from '@/api/myProduct';\nimport { listItem } from '@/api/listProduct';\nimport { deleteProduct } from \"@/api/deleteProduct\"; // eslint-disable-next-line\n\nimport { ElMessage, ElMessageBox } from 'element-plus';\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const form = ref();\n    const blankForm = {\n      product_name: '',\n      product_version: '',\n      inventory: ''\n    };\n    form.value = blankForm;\n\n    const initProductData = async () => {\n      itemsData.value = [];\n      itemsData.value = await myItems(localStorage.getItem('token'));\n    }; // listItem\n    // eslint-disable-next-line\n\n\n    const store = useStore();\n\n    const handleList = async () => {\n      // console.log(form)\n      // store.dispatch('app/listItem', form.value)\n      await initProductData();\n      listItem(form.value, localStorage.getItem('token')).then(() => {\n        // console.log('get returned' + res)\n        ElMessageBox.alert('提交成功！', {\n          confirmButtonText: 'OK',\n          callback: () => {\n            form.value = {\n              product_name: '',\n              product_version: '',\n              inventory: ''\n            };\n            ElMessage({\n              type: 'info',\n              message: '已更新列表'\n            });\n          }\n        });\n      }).catch(err => {\n        alert(err);\n      });\n      setTimeout(initProductData, 1);\n    };\n\n    const itemsData = ref([]);\n    onMounted(async () => {\n      const y = await myItems(localStorage.getItem('token'));\n      itemsData.value = y;\n    });\n\n    const handleDelete = async row => {\n      console.log(row);\n      deleteProduct({\n        token: localStorage.getItem('token'),\n        productId: row.productId\n      });\n      ElMessage({\n        type: 'info',\n        message: '已删除'\n      });\n      setTimeout(initProductData,