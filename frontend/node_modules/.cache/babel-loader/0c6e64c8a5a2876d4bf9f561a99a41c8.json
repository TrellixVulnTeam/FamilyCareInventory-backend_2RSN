{"ast":null,"code":"import { useStore } from 'vuex';\nimport { addBuy } from '@/api/addBuy'; // eslint-disable-next-line\n\nimport { ElMessage, ElMessageBox } from 'element-plus';\nimport { onMounted, ref } from \"vue\";\nimport { myOrders } from \"@/api/myOrder\";\nimport { orderDetail } from \"@/api/orderDetail\";\nimport { deleteOrder } from \"@/api/deleteOrder\";\nexport default {\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const orderForm = ref({\n      products: [{\n        product_name: '',\n        product_version: '',\n        quantity: '',\n        price: ''\n      }],\n      note: '',\n      trans_time: '',\n      flag: '',\n      total: '',\n      customer_name: ''\n    });\n\n    const addorder = async () => {\n      // console.log(form)\n      // store.dispatch('app/listItem', form.value)\n      await initOrderData();\n      addBuy(orderForm.value, localStorage.getItem('token')).then(() => {\n        // console.log('get returned' + res)\n        ElMessageBox.alert('提交成功！', {\n          confirmButtonText: 'OK',\n          callback: () => {\n            orderForm.value = {\n              products: [{\n                product_name: '',\n                product_version: '',\n                quantity: '',\n                price: ''\n              }],\n              note: '',\n              trans_time: '',\n              flag: '',\n              total: '',\n              customer: ''\n            };\n            ElMessage({\n              type: 'info',\n              message: '已更新列表'\n            });\n          }\n        });\n      }).catch(err => {\n        alert(err);\n      });\n      setTimeout(initOrderData, 1);\n    };\n\n    const addOrderProduct = async () => {\n      orderForm.value.products.push({\n        product_name: '',\n        product_version: '',\n        quantity: '',\n        price: '',\n        key: Date.now()\n      });\n    };\n\n    const removeOrderProduct = product => {\n      var index = orderForm.value.products.indexOf(product);\n\n      if (index !== -1) {\n        orderForm.value.products.splice(index, 1);\n      }\n    };\n\n    const resetOrderForm = async () => {\n      orderForm.value = {\n        products: [{\n          product_name: '',\n          product_version: '',\n          quantity: '',\n          price: ''\n        }],\n        note: '',\n        trans_time: '',\n        flag: '',\n        total: '',\n        customer_name: ''\n      };\n    }; // listItem\n    // eslint-disable-next-line\n\n\n    const store = useStore();\n    const ordersData = ref([]);\n    onMounted(async () => {\n      const y = await myOrders(localStorage.getItem('token'));\n      ordersData.value = y;\n    });\n    const showDialog = ref(false);\n    const dialogData = ref([]);\n\n    const handleDetail = async row => {\n      // console.log(1111)\n      // eslint-disable-next-line\n      const x = await orderDetail(row.orderId, localStorage.getItem('token'));\n      showDialog.value = true;\n      dialogData.value = x;\n    };\n\n    const initOrderData = async () => {\n      ordersData.value = [];\n      ordersData.value = await myOrders(localStorage.getItem('token'));\n    };\n\n    const handleDelete = async row => {\n      console.log(row);\n      deleteOrder({\n        token: localStorage.getItem('token'),\n        orderId: row.orderId\n      });\n      ElMessage({\n        type: 'info',\n        message: '已删除'\n      });\n      setTimeout(initOrderData, 1);\n    };\n\n    const __returned__ = {\n      orderForm,\n      addorder,\n      addOrderProduct,\n      removeOrderProduct,\n      resetOrderForm,\n      store,\n      ordersData,\n      showDialog,\n      dialogData,\n      handleDetail,\n      initOrderData,\n      handleDelete,\n      useStore,\n      addBuy,\n      ElMessage,\n      ElMessageBox,\n      onMounted,\n      ref,\n      myOrders,\n      orderDetail,\n      deleteOrder\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAoGA,SAASA,QAAT,QAAyB,MAAzB;AACA,SAASC,MAAT,QAAuB,cAAvB,C,CACA;;AACA,SAASC,SAAT,EAAoBC,YAApB,QAAwC,cAAxC;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AACA,SAAQC,QAAR,QAAuB,eAAvB;AACA,SAAQC,WAAR,QAA0B,mBAA1B;AACA,SAAQC,WAAR,QAA0B,mBAA1B;;;;;;IAEA,MAAMC,SAAS,GAAGJ,GAAG,CAAC;MACpBK,QAAQ,EAAE,CAAC;QACTC,YAAY,EAAE,EADL;QAETC,eAAe,EAAE,EAFR;QAGTC,QAAQ,EAAE,EAHD;QAITC,KAAK,EAAE;MAJE,CAAD,CADU;MAOpBC,IAAI,EAAE,EAPc;MAQpBC,UAAU,EAAE,EARQ;MASpBC,IAAI,EAAE,EATc;MAUpBC,KAAK,EAAE,EAVa;MAWpBC,aAAa,EAAE;IAXK,CAAD,CAArB;;IAcA,MAAMC,QAAQ,GAAG,YAAY;MAC3B;MACA;MACA,MAAMC,aAAa,EAAnB;MACApB,MAAM,CAACQ,SAAS,CAACa,KAAX,EAAkBC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAlB,CAAN,CAAuDC,IAAvD,CAA4D,MAAM;QAChE;QACAtB,YAAY,CAACuB,KAAb,CAAmB,OAAnB,EAA4B;UAC1BC,iBAAiB,EAAE,IADO;UAE1BC,QAAQ,EAAE,MAAM;YACdnB,SAAS,CAACa,KAAV,GAAkB;cAChBZ,QAAQ,EAAE,CAAC;gBACTC,YAAY,EAAE,EADL;gBAETC,eAAe,EAAE,EAFR;gBAGTC,QAAQ,EAAE,EAHD;gBAITC,KAAK,EAAE;cAJE,CAAD,CADM;cAOhBC,IAAI,EAAE,EAPU;cAQhBC,UAAU,EAAE,EARI;cAShBC,IAAI,EAAE,EATU;cAUhBC,KAAK,EAAE,EAVS;cAWhBW,QAAQ,EAAE;YAXM,CAAlB;YAaA3B,SAAS,CAAC;cACR4B,IAAI,EAAE,MADE;cAERC,OAAO,EAAE;YAFD,CAAD,CAAT;UAID;QApByB,CAA5B;MAsBD,CAxBD,EAwBGC,KAxBH,CAwBUC,GAAD,IAAS;QAChBP,KAAK,CAACO,GAAD,CAAL;MACD,CA1BD;MA2BAC,UAAU,CAACb,aAAD,EAAgB,CAAhB,CAAV;IACD,CAhCD;;IAkCA,MAAMc,eAAe,GAAG,YAAU;MAChC1B,SAAS,CAACa,KAAV,CAAgBZ,QAAhB,CAAyB0B,IAAzB,CAA8B;QAC5BzB,YAAY,EAAE,EADc;QAE5BC,eAAe,EAAE,EAFW;QAG5BC,QAAQ,EAAE,EAHkB;QAI5BC,KAAK,EAAE,EAJqB;QAK5BuB,GAAG,EAAEC,IAAI,CAACC,GAAL;MALuB,CAA9B;IAMG,CAPL;;IASA,MAAMC,kBAAkB,GAAIC,OAAD,IAAa;MACtC,IAAIC,KAAK,GAAGjC,SAAS,CAACa,KAAV,CAAgBZ,QAAhB,CAAyBiC,OAAzB,CAAiCF,OAAjC,CAAZ;;MACA,IAAIC,KAAK,KAAK,CAAC,CAAf,EAAkB;QAChBjC,SAAS,CAACa,KAAV,CAAgBZ,QAAhB,CAAyBkC,MAAzB,CAAgCF,KAAhC,EAAuC,CAAvC;MACD;IACF,CALD;;IAOA,MAAMG,cAAc,GAAG,YAAW;MAChCpC,SAAS,CAACa,KAAV,GAAkB;QAChBZ,QAAQ,EAAE,CAAC;UACTC,YAAY,EAAE,EADL;UAETC,eAAe,EAAE,EAFR;UAGTC,QAAQ,EAAE,EAHD;UAITC,KAAK,EAAE;QAJE,CAAD,C